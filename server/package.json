{
    "name": "got2do-server",
    "version": "0.0.0",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "nest start --watch",
        "build": "nest build",
        "start": "pnpm build && node dist/main",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "seed": "ts-node -r tsconfig-paths/register ./src/database/run-seeder.ts"
    },
    "dependencies": {
        "@liaoliaots/nestjs-redis": "^10.0.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/typeorm": "^10.0.2",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "google-auth-library": "^9.15.0",
        "ioredis": "^5.4.1",
        "ms": "^2.1.3",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "typeorm-extension": "^3.6.3",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/swagger": "^8.0.7",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/ms": "^0.7.34",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.16.0",
        "globals": "^15.12.0",
        "jest": "^29.5.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.16.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}